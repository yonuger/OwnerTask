<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.young.ownertask">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.RegisterActivity" />
        <activity android:name=".ui.StartUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.home.HomeActivity" />
        <activity android:name=".ui.group.GroupActivity" />
        <activity android:name=".ui.list.ListActivity" />
        <activity android:name=".ui.profile.ProfileActivity" />
        <activity android:name=".ui.timeline.TimeLineActivity" />
        <activity android:name=".ui.setting.SettingActivity" />
        <activity android:name=".ui.calendar.CalendarActivity"></activity>
        <activity android:name=".ui.add.view.AddActivity_"/>
    </application>

</manifest>